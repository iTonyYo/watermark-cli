{
  "bin": {
    "watermark": "cli.js"
  },
  "files": [
    "cli.js"
  ],
  "scripts": {
    "start": "node ./cli ./test/mark.png ./test/paper.png --scale 0.3 --opacity 0.8 --position center --gap 18",
    "clean": "node ./scripts/clean",
    "reprepare": "node ./scripts/re-prepare",
    "lint": "npx eslint --cache cli.js",
    "license": "node ./scripts/license",
    "changelog": "npx auto-changelog -p"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "@oopsunome/watermark": "^1.0.6",
    "lodash": "^4.17.15",
    "meow": "^8.0.0",
    "update-notifier": "^6.0.0"
  },
  "private": false,
  "name": "@oopsunome/watermark-cli",
  "description": "从命令行给图片贴水印。",
  "version": "1.0.5",
  "license": "MIT",
  "author": {
    "name": "iyowei",
    "email": "swzyocowboy@icloud.com",
    "url": "https://github.com/iTonyYo"
  },
  "keywords": [
    "水印",
    "watermark",
    "cli",
    "工具",
    "tool",
    "oopsunome"
  ],
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base"
    ],
    "parser": "babel-eslint",
    "rules": {
      "no-console": 0
    }
  },
  "eslintIgnore": [
    "benchmark/**",
    "docs/**",
    "*-test.js",
    "esm/**",
    "test/**",
    ".github/**",
    ".vscode/**",
    "scripts/**",
    "coverage/**"
  ],
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog-template.hbs",
    "unreleased": true,
    "commitLimit": false,
    "includeBranch": [
      "master"
    ]
  },
  "gitignore": [
    "macOS",
    "Windows",
    "Linux",
    "Node",
    "VisualStudioCode",
    "SublimeText",
    "CVS",
    "Diff",
    "Vim",
    "TortoiseGit",
    "Diff",
    "Patch"
  ],
  "repository": {
    "url": "https://github.com/iTonyYo/watermark-cli.git",
    "type": "git"
  },
  "homepage": "https://github.com/iTonyYo/watermark-cli",
  "bugs": "https://github.com/iTonyYo/watermark-cli/issues",
  "devDependencies": {
    "auto-changelog": "^1.15.0",
    "babel-eslint": "^10.0.3",
    "chalk": "^2.4.2",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "execa": "^2.0.4",
    "license-checker": "^25.0.1",
    "listr": "^0.14.3",
    "make-dir": "^3.0.0",
    "trash": "^6.0.0",
    "write-file-atomic": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
